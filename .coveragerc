[run]
source =
    auth_app
    development
    tournaments
omit =
    env/*
    */test*
    */__init__.py
    */migrations/*
    */apps.py
    */models.py

[report]
exclude_lines =
    pragma: no cover
    def setUp
    def test_
    return raw_input
    print
    if __name__ == '__main__':
    def suite
